&lt;ul style=&quot;margin-bottom:11.0px&quot;&gt;
	&lt;li&gt;&lt;span style=&quot;font-size:11.0pt&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family:Calibri, sans-serif&quot;&gt;&lt;span style=&quot;font-size:10.0pt&quot;&gt;&lt;span&gt;&lt;span style=&quot;font-family:&amp;apos;TeamViewer Sans&amp;apos;&quot;&gt;Degree in computer science, information systems, applied computer science, or similar education with IT-related background&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
	&lt;li&gt;&lt;span style=&quot;font-size:11.0pt&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family:Calibri, sans-serif&quot;&gt;&lt;span style=&quot;font-size:10.0pt&quot;&gt;&lt;span&gt;&lt;span style=&quot;font-family:&amp;apos;TeamViewer Sans&amp;apos;&quot;&gt;Fluency in&amp;#xa0;English and German&amp;#xa0;is mandatory&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
	&lt;li&gt;&lt;span style=&quot;font-size:11.0pt&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family:Calibri, sans-serif&quot;&gt;&lt;span style=&quot;font-size:10.0pt&quot;&gt;&lt;span&gt;&lt;span style=&quot;font-family:&amp;apos;TeamViewer Sans&amp;apos;&quot;&gt;Experience working in close customer collaboration, and the ability to effectively communicate at all levels&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
	&lt;li&gt;&lt;span style=&quot;font-size:11.0pt&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family:Calibri, sans-serif&quot;&gt;&lt;span style=&quot;font-size:10.0pt&quot;&gt;&lt;span&gt;&lt;span style=&quot;font-family:&amp;apos;TeamViewer Sans&amp;apos;&quot;&gt;Experience advocating for customers and the ability to foster and cultivate relationships with key customers&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
	&lt;li&gt;&lt;span style=&quot;font-size:11.0pt&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family:Calibri, sans-serif&quot;&gt;&lt;span style=&quot;font-size:10.0pt&quot;&gt;&lt;span&gt;&lt;span style=&quot;font-family:&amp;apos;TeamViewer Sans&amp;apos;&quot;&gt;Experience with managing projects and programs&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
	&lt;li&gt;&lt;span style=&quot;font-size:11.0pt&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family:Calibri, sans-serif&quot;&gt;&lt;span style=&quot;font-size:10.0pt&quot;&gt;&lt;span&gt;&lt;span style=&quot;font-family:&amp;apos;TeamViewer Sans&amp;apos;&quot;&gt;Related experience in delivering enterprise software solutions is a plus&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
	&lt;li&gt;&lt;span style=&quot;font-size:11.0pt&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family:Calibri, sans-serif&quot;&gt;&lt;span style=&quot;font-size:10.0pt&quot;&gt;&lt;span&gt;&lt;span style=&quot;font-family:&amp;apos;TeamViewer Sans&amp;apos;&quot;&gt;Available to visit (existing and potential) enterprise customers&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
